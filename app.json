{
  "expo": {
    "name": "Airsense",
    "slug": "airsense",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/airsense-logo.png",
    "scheme": "airsense",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0A1F1C",
        "foregroundImage": "./assets/images/airsense-icon.png",
        "backgroundImage": "./assets/images/airsense-icon.png",
        "monochromeImage": "./assets/images/airsense-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.airsense"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/airsense-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/airsense-logo.png",
          "imageWidth": 128,
          "resizeMode": "contain",
          "backgroundColor": "#0A1F1C",
          "dark": {
            "backgroundColor": "#0A1F1C"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
